
#include <stdio.h>

int main()
{
    int x;
    printf("Enter the marks");
    scanf("%d",&x);
    if(x >= 90 && x <= 100)
    {
        printf("Grade A");
    }
    else if(x >= 75 && x <= 89)
    {
        printf("Grade B");
    }
    else if(x >= 60 && x <= 74)
    {
        printf("Grade C");
    }
    else if(x >= 50 && x <= 59)
    {
        printf("Grade D");
    }
    else
    {
        printf("Grade D");
    }

    return 0;
}
